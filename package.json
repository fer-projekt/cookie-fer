{
  "name": "fer-cookie-bot",
  "version": "1.2.7",
  "description": "FerCookieBot is a JavaScript solution designed for website owners to manage user consents for cookies and tracking technologies, ensuring compliance with data protection regulations like GDPR and CCPA. It now seamlessly integrates with both Google Consent Mode v2 and Facebook Pixel, offering dynamic consent handling and a respectful, legal approach to user data across major advertising platforms.",
  "main": "fer-cookiebot.js",
  "scripts": {
    "build-js": "webpack --mode production",
    "build-css": "sass --no-source-map css/fer-cookiebot.scss dist/fer-cookiebot.min.css --style compressed",
    "build": "npm run build-js && npm run build-css",
    "watch-js": "webpack --watch --mode development",
    "watch-css": "sass --no-source-map --watch  css/fer-cookiebot.scss:dist/fer-cookiebot.min.css --style compressed",
    "watch": "npm-run-all --parallel watch-*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fer-projekt/cookiebot.git"
  },
  "keywords": [
    "Cookiebot",
    "Google",
    "Consent",
    "v2",
    "Facebook",
    "Pixel",
    "JavaScript",
    "Lightweight",
    "Privacy",
    "Translations"
  ],
  "author": "Fer Projekt",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fer-projekt/cookiebot/issues"
  },
  "homepage": "https://github.com/fer-projekt/cookiebot#readme",
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/preset-env": "^7.24.0",
    "babel-loader": "^9.1.3",
    "npm-run-all": "^4.1.5",
    "sass": "^1.71.1",
    "sass-loader": "^14.1.1",
    "terser-webpack-plugin": "^5.3.10",
    "webpack": "^5.90.3",
    "webpack-cli": "^5.1.4"
  }
}
